SELECT 
	average_salary,
	branch_code,
	payroll_year
FROM t_Lenka_Oumrtova_project_SQL_primary_final tlopspf
WHERE statistical_variable = '5958'
	AND payroll_year >= '2006' AND payroll_year <= '2018'
	AND branch_code = 'A'
GROUP BY payroll_year 
;

